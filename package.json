{
  "name": "fredericksburg-tourism-map",
  "version": "1.0.0",
  "description": "Interactive tourism map for Fredericksburg, Texas",
  "main": "src/index.html",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "ionic:build": "npm run build && npx cap sync",
    "lint": "eslint src/js/**/*.js",
    "lint:css": "stylelint 'src/css/**/*.css'",
    "test": "jest",
    "optimize-images": "node scripts/optimize-images.js",
    "generate-critical-css": "node scripts/critical-css.js",
    "capacitor:sync:before": "echo 'Sync starting'",
    "capacitor:sync:after": "echo 'Sync completed'"
  },
  "dependencies": {
    "@capacitor/core": "5.7.8",
    "@capacitor/app": "5.0.8",
    "@capacitor/haptics": "5.0.8",
    "@capacitor/keyboard": "5.0.8",
    "@capacitor/status-bar": "5.0.8",
    "@capacitor/ios": "5.7.8",
    "cordova": "12.0.0",
    "cordova-android": "12.0.0",
    "cordova-ios": "6.2.0",
    "ionic": "7.2.3",
    "mapbox-gl": "^2.12.0",
    "leaflet": "^1.9.3"
  },
  "devDependencies": {
    "@capacitor/cli": "5.7.8",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "critical": "^5.0.4",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.32.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "imagemin": "^8.0.1",
    "jest": "^29.4.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ]
  },
  "capacitor": {
    "ios": {
      "minVersion": "13.0"
    },
    "android": {
      "minVersion": "21"
    }
  }
}
